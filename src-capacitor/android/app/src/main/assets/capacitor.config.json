{
  "appId": "com.devbez.vidashlive",
  "appName": "Vidash Live",
  "bundledWebRuntime": false,
  "npmClient": "npm",
  "webDir": "www",
  "server": {
    "hostname": "vidash.live:443",
    "androidScheme": "https"
  },
  "plugins": {
    "SplashScreen": {
      "launchShowDuration": 3000,
      "androidScaleType": "CENTER_CROP",
      "spinnerColor": "#444851",
      "backgroundColor": "#2C2D34",
      "splashFullScreen": true
    }
  }
}